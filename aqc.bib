	language = {en},
	urldate = {2024-11-20},
	publisher = {arXiv},
	author = {Greiter, Martin and Wilczek, Frank},
	month = nov,
	year = {2022},
	note = {arXiv:2210.02530 [cond-mat]},
	keywords = {Quantum Physics, Condensed Matter - Strongly Correlated Electrons, Mathematical Physics, Mathematics - Mathematical Physics},
	annote = {Comment: 40 pages, 7 figures. arXiv admin note: text overlap with arXiv:1109.6104},
