	note = {arXiv:2211.03855 [quant-ph]},
	keywords = {Quantum Physics, Mathematics - Category Theory},
	pages = {415301},
	file = {PDF:/home/jkr/Zotero/storage/MDXHCVR7/Ahmadi and Kissinger - 2023 - The ZX-calculus as a Language for Topological Quantum Computation.pdf:application/pdf},
